{
  "title": "Read More Article Inline",
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "coco/read-more-inline",
  "version": "1.0",
  "description": "Insert a Related Article Inline (Read Next aligned to left) with the paragraphs, on the left side of the content.",
  "category": "widgets",
  "icon": "table",
  "supports": {
    "renaming": false
  },
  "attributes": {
    "source": {
      "type": "string",
      "default": "category"
    },
    "category_id": {
      "type": "number",
      "default": ""
    },
    "tag_id": {
      "type": "number",
      "default": ""
    },
    "post_id": {
      "type": "number",
      "default": null
    },
    "parent_post_id": {
      "type": "number",
      "default": null
    }
  },
  "example": {
    "attributes": {
      "source": "category",
      "category_id": 3,
      "topic_id": "",
      "post_id": "",
      "parent_post_id": 5
    }
  },
	"editorScript": "file:./index.js",
	"render": "file:./render.php",
	"style": "file:./style-index.css"
}
